@using EntityLayer.Concrete
@model MainModel
@{
    ViewData["Title"] = "Home Page";
}
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/UserLayout.cshtml";
}

<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Quicky - Minimal eCommerce HTML Template</title>
    <meta name="robots" content="noindex, follow" />
    <meta name="description" content="Quicky is a powerful & a full-fledged HTML template for your store that stand out from the crowd. 6 unique homepages included in this template. You can use it for any kinda eCommerce site.">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/web/quicky/assets/images/favicon.ico">

    <!-- CSS
    ============================================ -->
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/bootstrap.min.css">
    <!-- Material Design Iconic Font -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/material-design-iconic-font.css">
    <!-- Simple Line Icons -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/simple-line-icons.css">
    <!-- Local Font -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/font.css">
    <!-- Slick CSS -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/slick.css">
    <!-- Animation -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/animate.min.css">
    <!-- jQuery Ui -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/jquery-ui.min.css">
    <!-- Nice Select -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/nice-select.css">
    <!-- Timecircles -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/timecircles.css">

    <!-- Main Style CSS -->
    <link rel="stylesheet" href="~/web/quicky/assets/css/style.css">

</head>

<body class="template-color-1 font-family-01">

    <div class="main-wrapper">


        <!-- Begin Slider Area -->
        @await Html.PartialAsync("SliderPartial")
        <!-- Slider Area End Here -->
        <!-- Begin Banner Area -->
        <div class="banner-area pt-100">
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-6 custom-xxs-col">
                        <div class="banner-item">
                            <div class="banner-img">
                                <a href="javascript:void(0)">
                                    <img src="~/web/quicky/assets/images/banner/p.jpg" alt="Quicky's Banner">
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-8 col-6 custom-xxs-col">
                        <div class="row banner-wrap">
                            <div class="col-md-6">
                                <div class="banner-item">
                                    <div class="banner-img">
                                        <a href="javascript:void(0)">
                                            <img src="~/web/quicky/assets/images/banner/p2.jpg" alt="Quicky's Banner">
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="banner-item">
                                    <div class="banner-img">
                                        <a href="javascript:void(0)">
                                            <img src="~/web/quicky/assets/images/banner/p3.jpg" alt="Quicky's Banner">
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row banner-wrap d-none d-md-block">
                            <div class="col-lg-12">
                                <div class="banner-item">
                                    <div class="banner-img">
                                        <a href="javascript:void(0)">
                                            <img src="~/web/quicky/assets/images/banner/p1.jpg" alt="Quicky's Banner">
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12 d-block d-md-none">
                        <div class="banner-item specific-banner_item">
                            <div class="banner-img">
                                <a href="javascript:void(0)">
                                    <img src="~/web/quicky/assets/images/banner/1-4.jpg" alt="Quicky's Banner">
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Banner Area End Here -->
        <!-- Begin Letest Blog Area -->
        @await Html.PartialAsync("TrendPartial")
        <!-- Letest Blog Area End Here -->
        <!-- Begin Banner Area Seven -->
        <div class="banner-area-7 pt-30 ml-6 mr-6 mb-3">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 mb-5">
                        <div class="section-title">
                            <h3 class="heading">Seriler</h3>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="banner-item">
                            <div class="banner-img">
                                <a href="javascript:void(0)">
                                    <img src="~/web/quicky/assets/images/banner/4-1.jpg" alt="Quicky's Banner">
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="banner-item">
                            <div class="banner-img">
                                <a href="javascript:void(0)">
                                    <img src="~/web/quicky/assets/images/banner/4-2.jpg" alt="Quicky's Banner">
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="banner-item">
                            <div class="banner-img">
                                <a href="javascript:void(0)">
                                    <img src="~/web/quicky/assets/images/banner/4-3.jpg" alt="Quicky's Banner">
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Banner Area Seven End Here -->
        <!-- Begin Letest Blog Area -->
        <div class="latest-blog_area pt-95 pb-90">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="section-title">
                            <h3 class="heading">Markalar</h3>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="quicky-element-carousel product-slider" data-slick-options='{
                            "slidesToShow": 3,
                            "slidesToScroll": 1,
                            "infinite": false,
                            "arrows": false,
                            "dots": false,
                            "spaceBetween": 30
                            }' data-slick-responsive='[
                            {"breakpoint":992, "settings": {
                            "slidesToShow": 2
                            }},
                            {"breakpoint":768, "settings": {
                            "slidesToShow": 2
                            }},
                            {"breakpoint":480, "settings": {
                            "slidesToShow": 1
                            }}
                        ]'>

                            <div class="blog-item">
                                <div class="blog-img">
                                    <a href="blog-details.html">
                                        <img src="~/web/quicky/assets/images/blog/medium-size/1.jpg" alt="Quicky's Blog">
                                    </a>
                                </div>
                                <div class="blog-content">
                                    <h3 class="heading">
                                        <a href="blog-details.html">
                                            Majestik
                                        </a>
                                    </h3>
                                </div>
                            </div>
                            <div class="blog-item">
                                <div class="blog-img">
                                    <a href="blog-details.html">
                                        <img src="~/web/quicky/assets/images/blog/medium-size/2.jpg" alt="Quicky's Blog">
                                    </a>
                                </div>
                                <div class="blog-content">
                                    <h3 class="heading">
                                        <a href="blog-details.html">
                                            Özpa
                                        </a>
                                    </h3>
                                </div>
                            </div>
                            <div class="blog-item">
                                <div class="blog-img">
                                    <a href="blog-details.html">
                                        <img src="~/web/quicky/assets/images/blog/medium-size/3.jpg" alt="Quicky's Blog">
                                    </a>
                                </div>
                                <div class="blog-content">
                                    <h3 class="heading">
                                        <a href="blog-details.html">
                                            Trrek
                                        </a>
                                    </h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Letest Blog Area End Here -->
        <!-- Scroll To Top Start -->
        <a class="scroll-to-top" href=""><i class="icon-arrow-up"></i></a>
        <!-- Scroll To Top End -->

    </div>

    <!-- JS
    ============================================ -->
    <!-- jQuery JS -->
    <script src="~/web/quicky/assets/js/vendor/jquery-3.6.0.min.js"></script>
    <script src="~/web/quicky/assets/js/vendor/jquery-migrate-3.3.2.min.js"></script>
    <!-- Modernizer JS -->
    <script src="~/web/quicky/assets/js/vendor/modernizr-3.11.2.min.js"></script>
    <!-- Bootstrap JS -->
    <script src="~/web/quicky/assets/js/vendor/bootstrap.bundle.min.js"></script>

    <!-- Slick Slider JS -->
    <script src="~/web/quicky/assets/js/plugins/slick.min.js"></script>
    <!-- Barrating JS -->
    <script src="~/web/quicky/assets/js/plugins/jquery.barrating.min.js"></script>
    <!-- Counterup JS -->
    <script src="~/web/quicky/assets/js/plugins/jquery.counterup.js"></script>
    <!-- Nice Select JS -->
    <script src="~/web/quicky/assets/js/plugins/jquery.nice-select.js"></script>
    <!-- Sticky Sidebar JS -->
    <script src="~/web/quicky/assets/js/plugins/jquery.sticky-sidebar.js"></script>
    <!-- Jquery-ui JS -->
    <script src="~/web/quicky/assets/js/plugins/jquery-ui.min.js"></script>
    <script src="~/web/quicky/assets/js/plugins/jquery.ui.touch-punch.min.js"></script>
    <!-- Theia Sticky Sidebar JS -->
    <script src="~/web/quicky/assets/js/plugins/theia-sticky-sidebar.min.js"></script>
    <!-- Waypoints JS -->
    <script src="~/web/quicky/assets/js/plugins/waypoints.min.js"></script>
    <!-- jQuery Zoom JS -->
    <script src="~/web/quicky/assets/js/plugins/jquery.zoom.min.js"></script>
    <!-- Timecircles JS -->
    <script src="~/web/quicky/assets/js/plugins/timecircles.js"></script>
    <!-- Ajax Mail JS -->
    <script src="~/web/quicky/assets/js/ajax-mail.js"></script>

    <!-- Main JS -->
    <script src="~/web/quicky/assets/js/main.js"></script>

</body>

</html>
